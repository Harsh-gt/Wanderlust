<%if(success.length){%>
<div
  class="alert alert-success alert-dismissable fade show col-6 offset-3"
  role="alert"
>
  <%=success%>
  <button
    type="button"
    class="btn-close position-absolute top-50 end-0 translate-middle-y me-2"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%> <%if(error.length){%>
<div
  class="alert alert-danger alert-dismissable fade show col-6 offset-3"
  role="alert"
>
  <%=error%>
  <button
    type="button"
    class="btn-close position-absolute top-50 end-0 translate-middle-y me-2"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%>
